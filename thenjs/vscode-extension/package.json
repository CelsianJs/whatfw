{
  "name": "thenjs",
  "displayName": "ThenJS + What Framework",
  "description": "Syntax highlighting, snippets, and JSX support for ThenJS and What Framework â€” signals-based UI with compiler-first JSX",
  "version": "0.1.0",
  "publisher": "zvndev",
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "keywords": [
    "thenjs",
    "what-framework",
    "signals",
    "jsx",
    "tsx",
    "islands",
    "ssr",
    "reactive",
    "framework"
  ],
  "galleryBanner": {
    "color": "#1a1a2e",
    "theme": "dark"
  },
  "badges": [
    {
      "url": "https://img.shields.io/npm/v/what-framework?color=%232563eb&label=what-framework",
      "href": "https://www.npmjs.com/package/what-framework",
      "description": "What Framework on npm"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zvndev/thenjs"
  },
  "homepage": "https://whatfw.com",
  "icon": "icon.png",
  "activationEvents": [
    "onLanguage:typescriptreact",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:javascript"
  ],
  "main": "",
  "contributes": {
    "grammars": [
      {
        "scopeName": "what-framework.injection",
        "path": "./syntaxes/what-framework.tmLanguage.json",
        "injectTo": [
          "source.tsx",
          "source.jsx"
        ]
      },
      {
        "scopeName": "thenjs.injection",
        "path": "./syntaxes/thenjs.tmLanguage.json",
        "injectTo": [
          "source.tsx",
          "source.jsx",
          "source.ts",
          "source.js"
        ]
      }
    ],
    "snippets": [
      {
        "language": "typescriptreact",
        "path": "./snippets/what-framework.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/what-framework.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/thenjs.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/thenjs.json"
      }
    ],
    "languages": [
      {
        "id": "typescriptreact",
        "configuration": "./language-configuration.json"
      }
    ]
  }
}
